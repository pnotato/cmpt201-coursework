#define _GNU_SOURCE
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {

  char *line = NULL;
  size_t len = 0;
  ssize_t nread;

  printf("Please enter some text: ");
  nread = getline(&line, &len, stdin);

  char *saveptr;
  char *token = strtok_r(line, " ", saveptr);

  while (token != NULL) {
    printf("%s\n", token);
    token = strtok_r(NULL, " ", &saveptr);
  }
  free(line);
}
